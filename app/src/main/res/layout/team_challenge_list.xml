<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0e2845"

    tools:context="muna.munaleagueandroidapp.TeamChallengesFragment"
    android:orientation="horizontal">



    <TextView
        android:id="@+id/activity"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:text="Acctivity"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        />

    <TextView
        android:id="@+id/dateCompleted"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:text="Completion"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        />
    <TextView
        android:id="@+id/numberOfPlayers"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:text="#Players"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        />

    <TextView
        android:id="@+id/nameOfPlayers"
        android:layout_width="0dp"
        android:layout_weight="1"
        android:text="Players Name"
        android:layout_height="wrap_content"
        android:textColor="@android:color/white"
        />

</LinearLayout>
